<body class="animsition" id="body" style="font-family:Roboto">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <input value="@CurrentUser.UserAdmin.ROLE_ID" id="current-role-id" hidden />
        <input value="@CurrentUser.UserAdmin.USER_ID" id="current-user-id" hidden />

        @Html.Action("GetMenuLeftMobileAdmin", "MenuAdmin", new { area = "ADMIN" })

        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->

        @Html.Action("GetMenuLeftAdmin", "MenuAdmin", new { area = "ADMIN" })

        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->

            @Html.Partial("~/Areas/ADMIN/Views/Shared/_Header_Desktop.cshtml")

            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                @RenderBody()

            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

        <!-- sound noti audio -->
        <audio controls="controls" id="notification-ring-tone-1" hidden>
            <source src="/SoundNotification/Notification-ring-tone.mp3" type="audio/mpeg" />
        </audio>
        <!-- end sound noti audio -->

    </div>
    <!-- modal scroll -->
    <div class="modal fade" id="modal-right" tabindex="-1" role="dialog" aria-labelledby="scrollmodalLabel" aria-hidden="true">
        <div class="modal-dialog right modal-lg" role="document">
            <div class="modal-content" id="modal-content-right">

            </div>
        </div>
    </div>

    <div class="modal fade" id="modal-center" tabindex="-1" role="dialog" aria-labelledby="scrollmodalLabel" aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content" id="modal-content-center">

            </div>
        </div>
    </div>


